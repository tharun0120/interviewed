{
  "name": "interviewed",
  "version": "1.0.0",
  "description": "interviewed. An interview platform!",
  "main": "server.js",
  "scripts": {
    "build": "cd ./client && npm install && npm run build",
    "test": "echo \"Error: no test specified\" && exit 1",
    "start": "cross-env NODE_ENV=PRODUCTION npm run build && node server",
    "client": "npm run start --prefix client",
    "server": "cross-env NODE_ENV=DEV nodemon server",
    "dev": "concurrently \"npm run client\" \"npm run server\""
  },
  "keywords": [
    "azure",
    "interview",
    "blobs"
  ],
  "author": "Tharun Kumar",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "body-parser": "^1.19.1",
    "concurrently": "^7.0.0",
    "cors": "^2.8.5",
    "cross-env": "^7.0.3",
    "dotenv": "^14.3.2",
    "express": "^4.17.2",
    "formik": "^2.2.9",
    "jsonwebtoken": "^8.5.1",
    "mongoose": "^6.1.8",
    "morgan": "^1.10.0",
    "nodemon": "^2.0.15",
    "path": "^0.12.7",
    "uuid": "^8.3.2",
    "validator": "^13.7.0",
    "yup": "^0.32.11"
  }
}
